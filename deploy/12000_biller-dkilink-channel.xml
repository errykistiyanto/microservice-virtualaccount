    <channel class="org.jpos.iso.channel.ASCIIChannel" logger="Q2" packager="co.id.middleware.router.iso.ISO87APackager">

    <property name="host" value="localhost"/>
    <property name="port" value="12000"/>
        
    </channel>
    <in>biller-dkilink-channel-in</in>
    <out>biller-dkilink-channel-out</out>
    <ready>biller-dkilink-channel.ready</ready>
